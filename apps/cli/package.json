{
  "name": "ai-stack-cli",
  "version": "1.2.0",
  "description": "Interactive CLI to scaffold AI Stack projects with Next.js, FastAPI, or TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "create-ai-stack-starter": "./dist/index.js"
  },
  "files": [
    "dist",
    "template"
  ],
  "scripts": {
    "prebuild": "node scripts/prepare-template.js",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "nextjs",
    "fastapi",
    "rag",
    "cli",
    "scaffold",
    "boilerplate",
    "typescript",
    "openai",
    "anthropic",
    "vercel-ai-sdk",
    "drizzle",
    "better-auth",
    "create-app"
  ],
  "author": "AI Stack Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/princepal9120/ai-stack.git"
  },
  "homepage": "https://ai-stack.dev",
  "dependencies": {
    "commander": "^12.1.0",
    "prompts": "^2.4.2",
    "chalk": "^5.4.1",
    "ora": "^8.1.1",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "gradient-string": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^22.10.2",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
