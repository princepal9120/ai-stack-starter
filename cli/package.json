{
  "name": "create-ai-stack",
  "version": "1.0.0",
  "description": "Interactive CLI to scaffold AI Stack FastAPI projects",
  "type": "module",
  "bin": {
    "create-ai-stack": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "fastapi",
    "rag",
    "cli",
    "scaffold",
    "boilerplate",
    "typescript"
  ],
  "author": "AI Stack Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "prompts": "^2.4.2",
    "chalk": "^5.4.1",
    "ora": "^8.1.1",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "gradient-string": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
